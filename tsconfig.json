{
	"extends": "@tsconfig/svelte/tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"inlineSources": true,
		"module": "ESNext",
		"target": "es6",
		"allowJs": true,
		"noImplicitAny": true,
		"moduleResolution": "node",
		"importHelpers": true,
		"lib": ["dom", "es5", "scripthost", "es2019"],
		"types": ["svelte"]
	},
	"include": ["**/*.ts"]
}
